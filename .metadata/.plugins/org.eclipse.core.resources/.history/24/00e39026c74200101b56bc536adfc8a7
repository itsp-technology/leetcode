package day06;

public class RemoveDuplicatFromSortedArray {

	public static void main(String[] args) {
		int[] sortedArray = {0,0,1,1,1,2,2,3,3,4};
		System.out.println(countUniquEle(sortedArray));

	}
	
	public static int countUniquEle(int[] sa) {
		int k = 1;
		for(int i = 0; i<sa.length; i++) {
			if(sa[i] != sa[k-1]) {
			sa[k]=	sa[i];
			k++;
			}
		}
		return k;
	}

}
